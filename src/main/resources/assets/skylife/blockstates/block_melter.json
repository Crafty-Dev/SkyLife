{
  "multipart": [
    {
      "apply": {
        "model": "skylife:block/block_melter"
      },
      "when": {
        "energy": false,
        "upgraded": false
      }
    },
    {
      "apply": {
        "model": "skylife:block/block_melter_charged"
      },
      "when": {
        "energy": true,
        "upgraded": false
      }
    },
    {
      "apply": {
        "model": "skylife:block/block_melter_upgraded"
      },
      "when": {
        "energy": false,
        "upgraded": true
      }
    },
    {
      "apply": {
        "model": "skylife:block/block_melter_charged_upgraded"
      },
      "when": {
        "energy": true,
        "upgraded": true
      }
    }

  ]
}
{
  "multipart": [
    {
      "apply": {
        "model": "skylife:block/fluid_pump"
      },
      "when": {
        "energy": false,
        "upgraded": false
      }
    },
    {
      "apply": {
        "model": "skylife:block/fluid_pump_charged"
      },
      "when": {
        "energy": true,
        "upgraded": false
      }
    },
    {
      "apply": {
        "model": "skylife:block/fluid_pump_upgraded"
      },
      "when": {
        "energy": false,
        "upgraded": true
      }
    },
    {
      "apply": {
        "model": "skylife:block/fluid_pump_charged_upgraded"
      },
      "when": {
        "energy": true,
        "upgraded": true
      }
    }
  ]
}